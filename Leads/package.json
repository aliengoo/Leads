{
  "name": "leads",
  "version": "1.0.0",
  "description": "Leads",
  "private": true,
  "workflow": {
    "_description": "workflow: your workflow configuration",
    "client": {
      "_description": "client: specifically related to the client-side, e.g. the angularjs code",
      "temp": {
        "_description": "temp: where temporary build files should be stored",
        "root": ".temp"
      },
      "src": {
        "root": "client",
        "_description": "src: roots of your source code",
        "js": {
          "root": "client",
          "outputFileName": "leads.js"
        },
        "styles": {
          "root": "client/styles",
          "outputFileName": "leads.css"
        },
        "index": {
          "templateFileUri": "client/index.tmpl"
        }
      },
      "angular": {
        "_description": "Angular configuration, where the mainModule is the root module for your application",
        "mainModule": "leads",
        "templates": {
          "_description": "angular.templates: where templates are stored",
          "module": "leads.templates"
        }
      },
      "fontLocation": {
        "_description": "You may need to change the font path in CSS files - this does a simple find and replace in, e.g. bootstrap.css",
        "find": "../fonts/",
        "replaceWith": "./"
      },
      "vendor": {
        "src": {
          "_description": "additionalFiles: Include files not already include by main-bower-files",
          "additionalFiles": [],
          "js": {
            "outputFileName": "vendor.js"
          },
          "styles": {
            "outputFileName": "vendor.css"
          }
        },
        "icomoon": {
          "root": "icomoon",
          "replace": {
            "thisValue": "fonts/",
            "withThis": "./"
          },
          "css": "icomoon.css",
          "json": "icomoon.json"
        }
      }
    }
  },
  "scripts": {
    "start": "gulp --silent --color --watch"
  },
  "keywords": [
    "testing",
    "angular"
  ],
  "author": "aliengoo",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "del": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-add-src": "^0.2.0",
    "gulp-angular-templatecache": "^1.7.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.1.0",
    "gulp-filter": "^2.0.2",
    "gulp-if": "^1.2.5",
    "gulp-inject": "^3.0.0",
    "gulp-jade": "^1.0.1",
    "gulp-less": "^3.0.3",
    "gulp-livereload": "^3.8.0",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-minify-css": "^1.2.0",
    "gulp-ng-annotate": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-print": "^1.1.0",
    "gulp-remove-empty-lines": "0.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-ruby-sass": "^1.0.5",
    "gulp-sass": "^2.0.4",
    "gulp-sftp": "^0.1.5",
    "gulp-shell": "^0.4.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-strip-comments": "^1.0.1",
    "gulp-template": "^3.0.0",
    "gulp-typescript": "^2.7.8",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.2.4",
    "gulp-webserver": "^0.9.1",
    "ip": "^1.0.1",
    "lazypipe": "^1.0.1",
    "lodash": "^3.10.1",
    "main-bower-files": "^2.8.2",
    "node-notifier": "^4.2.3",
    "node-sass": "^3.3.2",
    "phantomjs": "^1.9.18",
    "requirejs": "^2.1.20",
    "rewire": "^2.3.4",
    "run-sequence": "^1.1.2",
    "yargs": "^3.15.0"
  }
}
